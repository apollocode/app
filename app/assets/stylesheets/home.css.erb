
body {
  font-family: 'Roboto', sans-serif;
}

#flash {
  position: absolute;
  top: 27px;
  left: 467px;
  right: 146px;
}
#brand {
  background: url('<%= asset_path("logo-bg.png") %>') top left no-repeat;
  position: absolute;
  top: 23px;
  left: 0;
  width: 398px;
  height: 55px;
  background-size: cover;
  text-align: right;
}
#brand img {
  height: 43px;
  width: auto;
  position: relative;
  right: 40px;
  top: 4px;
}
#brand span {
  color:#76716E;
  text-transform: uppercase;
  position: relative;
  right: 40px;
  font-weight: 500;
  font-size: 25px;
  letter-spacing: 4px;
  top: 7px;
}

#first-div, #survey-div {
  background-image: url("<%= asset_path('bg_landing.jpg') %>");
  background-position: center center;
  background-repeat: no-repeat;
  background-attachment: fixed;
  background-size: cover;
  padding: 18% 6%;
  text-align: center
}
#value-prop {
  display: inline-block;
}

#value-prop .title {
  font-family: 'Roboto', sans-serif;
  text-transform: uppercase;
  font-size: 59px;
  border-top: 1px dotted rgba(250, 250, 250, 1);
  border-bottom: 1px dotted rgba(250, 250, 250, 1);
  line-height: 60px;
  font-weight: 400;
  margin: 5px 0px -5px;
  padding: 10px 25px;
  color:white;
  text-align: center
}

#value-prop .subtitle {
    font-family: 'Roboto', sans-serif;
  text-transform: uppercase;
  font-size: 32.5px;
  color: #fff;
  line-height: 40px;
  margin: 5px 0px -5px;
  padding: 10px 20px;
  font-weight: 300;
  text-shadow: none;
  text-align: center
}

#second-div {
  color: #332117;
  padding: 70px 30px;
}

#second-div p {
  font-size: 19px;
  text-align: center;
  padding-bottom: 25px;
}

#second-div .section-title {
  text-transform: uppercase;
}
.section-title {
  text-align: center;
  line-height: 40px;
  font-size: 40px;
  color: #332117;
}
.color {
  color: #f48a00;
}

#third-div {
  padding: 70px 0;
  background-color: #f48a00;
  text-align: center;
  color: white;
}

#third-div h2 {
  color: white;
  font-weight: 400;
}

#third-div p {
  font-size: 20px;
  letter-spacing: 1px;
  margin-top: 30px;
  font-weight: 300;
  color: #332117;
}

#third-div h4 {
  letter-spacing: 1.5px;
  height: 40px;
  font-size: 25px;
  font-weight: 400;
  margin-top: 60px;
  color: #332117;
}

#fourth-div {
  padding: 70px 0;
}
#fourth-div h2 {
  margin-bottom: 40px;
}
#fourth-div .item {
  margin-bottom: 30px
}
#fourth-div .fa {
  font-size: 40px;
  padding-left: 20px;
  padding-top: 10px;
}

#fourth-div .item span {
  font-size: 20px;
}




#footer {
  background-color: #f48a00;
  text-align: center;
  padding: 40px 0;
}

#footer h2 {
  color:white;
  font-weight: 400;
}

.call2action {
  margin-top:40px;
}

.call2action .control {
  padding-left: 20px;
  height: 47px;
  width: 325px;
  border-radius: 11px;
  font-size: 19px;
  color: #332117;
}

.call2action .control:focus {
  outline:none;
  background-color: #fff0c9;
}

.call2action .submit, #survey-div .submit {
  height: 47px;
  font-weight: 500;
  border-radius: 7px;
  font-size: 19px;
  border: 1px solid #E77800;
  padding: 10px 23px;
  margin-left: 27px;
  background: rgba(242,145,0,1);
  background: -moz-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(242,145,0,1)), color-stop(44%, rgba(230,106,39,1)), color-stop(100%, rgba(230,106,39,1)));
  background: -webkit-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: -o-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: -ms-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: linear-gradient(to bottom, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f29100', endColorstr='#e66a27', GradientType=0 );
}


.call2action .submit:hover, #survey-div .submit:hover {

  background: rgba(227,136,0,1);
  background: -moz-linear-gradient(top, rgba(227,136,0,1) 0%, rgba(235,98,0,1) 44%, rgba(235,98,0,1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(227,136,0,1)), color-stop(44%, rgba(235,98,0,1)), color-stop(100%, rgba(235,98,0,1)));
  background: -webkit-linear-gradient(top, rgba(227,136,0,1) 0%, rgba(235,98,0,1) 44%, rgba(235,98,0,1) 100%);
  background: -o-linear-gradient(top, rgba(227,136,0,1) 0%, rgba(235,98,0,1) 44%, rgba(235,98,0,1) 100%);
  background: -ms-linear-gradient(top, rgba(227,136,0,1) 0%, rgba(235,98,0,1) 44%, rgba(235,98,0,1) 100%);
  background: linear-gradient(to bottom, rgba(227,136,0,1) 0%, rgba(235,98,0,1) 44%, rgba(235,98,0,1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e38800', endColorstr='#eb6200', GradientType=0 );

}

/* Survey page styles */

#survey-div .padder {
  height:50px;
}

#survey-div .survey-header {
  color: white;
  background-color: rgba(0,0,0,0.5);
  padding: 1px 13px 12px 12px;
  margin-bottom: 30px;
}
#survey-div h1 {
  font-size: 52px;
}
#survey-div .survey-header p {
  font-size: 20px;
  font-weight: 300;
  letter-spacing: 0.4px;
}

#survey-div {
  padding:8% 0 2%;
  text-align: center
}

#survey-div .plan-container {
/*  display: inline-block;
  width: 362px;*/
}
#survey-div .header {
  width: 100%;
  height: 118px;
  margin-top: 0;
  background: rgba(255,140,0,1);
  background: -moz-linear-gradient(top, rgba(255,140,0,1) 0%, rgba(255,119,0,1) 93%, rgba(255,119,0,1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(255,140,0,1)), color-stop(93%, rgba(255,119,0,1)), color-stop(100%, rgba(255,119,0,1)));
  background: -webkit-linear-gradient(top, rgba(255,140,0,1) 0%, rgba(255,119,0,1) 93%, rgba(255,119,0,1) 100%);
  background: -o-linear-gradient(top, rgba(255,140,0,1) 0%, rgba(255,119,0,1) 93%, rgba(255,119,0,1) 100%);
  background: -ms-linear-gradient(top, rgba(255,140,0,1) 0%, rgba(255,119,0,1) 93%, rgba(255,119,0,1) 100%);
  background: linear-gradient(to bottom, rgba(255,140,0,1) 0%, rgba(255,119,0,1) 93%, rgba(255,119,0,1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ff8c00', endColorstr='#ff7700', GradientType=0 );
}

#survey-div h3 {
  width: 100%;
  padding: 12px 30px;
  height: 73px;
  margin-top: 0;
  text-align: center;
}
#survey-div h3.single-line {
  padding-top: 22px;
}

#survey-div .plan-wrapper {
  background-color: #fcf6ef;
  background-color: rgba(255,255,255,0.7);
  border: 1px solid #6b4f41;
  margin-top: 20px;
  text-align: left;
}

#survey-div .details {
list-style: none;
padding: 0 0 0 50px;
font-size: 18px;
margin:20px 0 10px;
}
#survey-div .fa {
  color:#FF9000;
  font-size:26px;
  margin-right: 7px;
  position: absolute;
  left: -32px;
  top: -1px;;
}

#survey-div .details ul{
  list-style: none;
  padding: 0 20px;
  font-size: 15px;
}

#survey-div .details > li {
  margin-top: 10px;
  position: relative;
}

#survey-div .price {
  font-family: 'Open Sans', sans-serif;
  width: 100%;
  text-align: center;
  font-size: 52px;
  color: white;
  font-weight: 200;
  position: relative;
  top: -25px;
  text-shadow: rgb(196, 91, 0) 3px 2px 4px;
}

#survey-div .price span {
  font-size: 32px;
}


#survey-div input[type="radio"] {
  display: none;
}

#survey-div .center {
  text-align: center;
  padding:15px 0;
}
#survey-div .click {
  display: inline-block;
  font-size: 18px;
  padding: 8px 47px;
  background-color: #999999;
}
#survey-div .click:hover, #survey-div input[type="radio"]:checked + label {
  background: rgba(242,145,0,1);
  background: -moz-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: -webkit-gradient(left top, left bottom, color-stop(0%, rgba(242,145,0,1)), color-stop(44%, rgba(230,106,39,1)), color-stop(100%, rgba(230,106,39,1)));
  background: -webkit-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: -o-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: -ms-linear-gradient(top, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  background: linear-gradient(to bottom, rgba(242,145,0,1) 0%, rgba(230,106,39,1) 44%, rgba(230,106,39,1) 100%);
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f29100', endColorstr='#e66a27', GradientType=0 );
}

#survey-div .details.first {
margin-bottom: 29px;
margin-top: 21px;
}
#survey-div .details.first > li {
  margin-top:12px;
}


.side-wrapper textarea {
  border-radius: 4px;
  min-height: 200px;
  width: 100%;
  font-size: 16px;
  padding: 3px 8px;
  background-color: rgba(255,255,255,0.7);
  margin-bottom: 30px;
}

.side-wrapper .caption {
  color: white;
  background-color: rgba(0,0,0,0.5);
  padding: 8px;
  margin-top: 31px;
  margin-bottom: 16px;
}

.side-wrapper .caption h4{
  font-size: 20px;
  font-weight: 300;
  letter-spacing: 0.4px;
}

#survey-div .plan-padder {
  height: 50px;
}












